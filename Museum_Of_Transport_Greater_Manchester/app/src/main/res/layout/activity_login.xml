<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
xmlns:android="http://schemas.android.com/apk/res/android"
android:orientation="vertical"
android:layout_width="fill_parent"
android:layout_height="fill_parent">

<fragment android:name="com.gmts.museum_of_transport_greater_manchester.Main"
    android:id="@+id/Main"
    android:layout_width="match_parent"
    android:layout_height="match_parent" />
<fragment android:name="com.gmts.museum_of_transport_greater_manchester.SplashFragment"
    android:id="@+id/splashFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent" />
<fragment android:name="com.facebook.widget.UserSettingsFragment"
    android:id="@+id/userSettingsFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent" />
</LinearLayout>

